 "devDependencies": {
  "@tailwindcss/typography": "^0.4.0",
  "lazysizes": "^5.3.0",
  "npm-run-all": "^4.1.5",
  "postcss-cli": "^8.0.0",
  "postcss-import": "^12.0.1",
  "quicklink": "^2.0.0",
  "react": "^16.14.0",
